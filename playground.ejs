<div class="form-group">
  <label class="control-label col-sm-2" for="ID">ID:</label>
  <div class="col-sm-10">
    <input type="text" name = "ID" value="<%= data.id %>" disabled>
  </div>
</div>

<div class="form-group">
  <label class="control-label col-sm-2" for="string">String:</label>
  <div class="col-sm-10">
    <input id="string" type="text" name = "string" value="<%=item.string%>" required>
  </div>
</div>
<div class="form-group">
  <label class="control-label col-sm-2" for="number">Integer:</label>
  <div class="col-sm-10">
    <input type="number" step="1" name = "integer" step="1" value="<%= item.integer %>" required>
  </div>
</div>
<div class="form-group">
  <label class="control-label col-sm-2" for="number">Float:</label>
  <div class="col-sm-10">
    <input type="number" class="form-control" name="float" step="0.01" value="<%= item.float %>" required>
  </div>
</div>
<div class="form-group">
  <label class="control-label col-sm-2" for="date">Date:</label>
  <div class="col-sm-10">
    <input type="date" class="form-control" name="date" value="<%= item.date %>" required>
  </div>
</div>
<div class="form-group">
  <label class="control-label col-sm-2" for="Boolean">Boolean:</label>
  <div class="col-sm-10">
    <select name="boolean" class="form-control">
      <option value="true"<%= item.boolean == "true" ? 'selected':'' %>>True</option>
    <option value="false"<%= !item.boolean == "false" ? 'selected':'' %>>False</option>
    </select>
  </div>
</div>


<div class="form-group">
  <label class="control-label col-sm-2" for="ID">ID:</label>
  <div class="col-sm-10">
    <input type="text" name = "ID" value="<%= item.id %>" disabled>
  </div>
</div>
